-(CGFloat)tableView:(UITableView *)tableView heightForRowAtIndexPath:(NSIndexPath *)indexPath
{
    switch (tableView.tag) {
        case TAG_TAB_DOWNLIST:
            return 62;
        case TAG_TAB_PLAYLIST:
            return 50;
        case TAG_TAB_QUICKDOWN:
            return 62;
        default:
            return 0;
    }
}

-(NSInteger)tableView:(UITableView *)tableView numberOfRowsInSection:(NSInteger)section
{
    switch (tableView.tag) {
        case TAG_TAB_PLAYLIST:
            return m_bookInfo.m_unCount;
            
        case TAG_TAB_DOWNLIST:
        {
            if (0 == (m_bookInfo.m_unCount % 5)) {
                return m_bookInfo.m_unCount / 5;
            }else {
                return (m_bookInfo.m_unCount / 5) + 1;
            }
        }
        case TAG_TAB_QUICKDOWN:
        {
            if (m_bookInfo.m_unCount <= 20) {
                return 1;
            }else {
                if (0 == ((m_bookInfo.m_unCount - 20) % 30)) {
                    return 1 + (m_bookInfo.m_unCount - 20) / 30;
                }else {
                    return 2 + (m_bookInfo.m_unCount - 20) / 30;
                }
            }
        }
        default:
            return 0;
    }
}

