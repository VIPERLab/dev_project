#import "KSViewController.h"

@interface KSViewController ()
{
    BOOL bInitWithFrame;
    CGRect frame;
}
@end

@implementation KSViewController

- (void)viewDidLoad
{
    [super viewDidLoad];
    if(bInitWithFrame)self.view.frame=frame;
}

- (id)initWithFrame:(CGRect)rc
{
    self=[super init];
    frame=rc;
    bInitWithFrame=YES;
    return self;
}

@end
