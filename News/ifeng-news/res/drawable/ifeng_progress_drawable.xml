<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
      <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dip" />
            <gradient android:startColor="#ff969696" android:endColor="#ff969696" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff969696" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient android:startColor="#ff881923" android:endColor="#ff881923" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>